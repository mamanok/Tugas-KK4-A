<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rahman.roomwordsample">

    <application
        android:name="com.rahman.roomwordsample.WordsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomWordSample">
        <activity
            android:name="com.rahman.roomwordsample.MainActivity"
            android:label="@string/RoomWordSample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rahman.roomwordsample.NewWordActivity" />
    </application>

</manifest>